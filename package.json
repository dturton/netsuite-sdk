{
  "name": "netsuite-sdk",
  "version": "0.1.0",
  "description": "TypeScript-first NetSuite REST API client with first-class SuiteQL support",
  "author": "David Turton",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dturton/netsuite-sdk.git"
  },
  "homepage": "https://github.com/dturton/netsuite-sdk#readme",
  "bugs": {
    "url": "https://github.com/dturton/netsuite-sdk/issues"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "engines": {
    "node": ">=20"
  },
  "files": [
    "dist/**",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist node_modules",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "oauth-1.0a": "^2.2.6"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "dotenv": "^17.3.1",
    "tsup": "^8.0.2",
    "typescript": "~5.5.4",
    "vitest": "^2.0.0"
  },
  "keywords": [
    "netsuite",
    "suiteql",
    "rest-api",
    "oauth",
    "typescript",
    "erp"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
